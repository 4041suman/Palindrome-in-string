class HelloWorld {
    public static void main(String[] args) {
        String str="121";
        
        boolean ispalindrome=true;
        
        for (int i=0, j=str.length()-1;i<j;i++,j--){
            
            if(str.charAt(i)==str.charAt(j))
            continue;
            
            ispalindrome=false;
            break;
            
        }
            
             System.out.println(ispalindrome ? " palindrome " : " not    palindrome");
        
     
    }
}
